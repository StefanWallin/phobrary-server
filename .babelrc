{
  "presets": [
    "./node_modules/@rails/webpacker/package/babel/preset.js",
    ["@babel/preset-env", {
      "modules": false,
      "targets": {
        "browsers": "> 1%",
        "uglify": true
      },
      "useBuiltIns": false
    }]
  ],

  "plugins": [
    "syntax-dynamic-import",
    "@babel/proposal-object-rest-spread",
    ["transform-class-properties", { "spec": true }]
  ]
}
